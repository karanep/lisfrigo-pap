<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="main.java.com.lisfrigo.hibernate.funcionario.bean.Funcionario" table="Funcionario">
		<id column="idfuncionario" name="idFunc" type="java.lang.Integer"> 
			<generator class="increment" />
		</id>
		<property column="nome" name="nome" type="java.lang.String" />
		<property column="iddepartamento" name="iddepartamento" type="java.lang.Integer"/>
</class>
</hibernate-mapping>
