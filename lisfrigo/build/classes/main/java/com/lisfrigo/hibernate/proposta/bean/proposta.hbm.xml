<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="main.java.com.lisfrigo.hibernate.proposta.bean.Proposta"
		table="Proposta">
		<id column="idproposta" name="idproposta" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property column="data" name="data" type="java.util.Date" />
		<property column="observacoes" name="observacoes" type="java.lang.String"/>
		<property column="idfunc" name="idfunc" type="java.lang.Integer"/>
		<property column="cliente" name="cliente" type="java.lang.Integer"/>
		<property column="prod"	name="prod" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
