<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="main.java.com.lisfrigo.hibernate.login.bean.Login"
		table="login">
		<id column="user" name="user" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property column="pass" name="pass" type="java.lang.String" />
		<property column="idFunc" name="idfunc" type="java.lang.Integer" />
		<property column="idDept" name="iddept" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
