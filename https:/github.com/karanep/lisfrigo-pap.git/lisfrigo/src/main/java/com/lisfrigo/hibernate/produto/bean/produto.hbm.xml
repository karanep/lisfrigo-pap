<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="main.java.com.lisfrigo.hibernate.produto.bean.Produto"
		table="Produto">
		<id name="idProduto" type="int" column="idProduto">
			<generator class="increment" />
		</id>
		<property column="Nome" name="nome" type="java.lang.String" />
		<property column="Descricao" name="descricao" type="java.lang.String" />
		<property column="Altura" name="altura" type="java.lang.Double" />
		<property column="Largura" name="largura" type="java.lang.Double" />
		<property column="Volume" name="volume" type="java.lang.Double" />
		<property column="Profundidade" name="profundidade" type="java.lang.Double" />
		<property column="Peso" name="peso" type="java.lang.Double" />
		<property column="Potencia" name="potencia" type="java.lang.Double" />
		<property column="Observacao" name="observacao" type="java.lang.String" />
		<property column="tipoprod" name="tipoprod" type="java.lang.Integer" />
		<property column="preco" name="preco" type="java.lang.Double"/>
	</class>


</hibernate-mapping>